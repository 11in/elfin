{
  "name": "elfin",
  "version": "1.6.0-alpha",
  "description": "11ty starter package.",
  "main": "index.js",
  "scripts": {
    "serve": "run-p assets:watch content:serve",
    "build": "run-s all:clean all:build",
    "deploy": "run-s assets:build content:build",
    "all:clean": "run-p content:clean assets:clean",
    "all:build": "run-s assets:build content:build",
    "all:production": "cross-env NODE_ENV=production npm run all:build",
    "all:watch": "run-p assets:watch content:watch",
    "content:clean": "rimraf ./dist",
    "content:build": "npx @11ty/eleventy --config=eleventy.config.js",
    "content:watch": "npm run content:build -- --watch",
    "content:serve": "npm run content:build -- --serve",
    "content:dev": "run-s assets:build content:serve",
    "content:debug": "DEBUG=Eleventy* content:build",
    "assets:clean": "rimraf ./content/assets ./content/_data/assets.json webpack.hash",
    "assets:build": "node tasks/webpack.js",
    "assets:watch": "webpack --watch",
    "preserve": "npm run assets:build",
    "preall:production": "npm run all:clean",
    "preassets:serve": "npm run assets:clean",
    "preassets:watch": "npm run assets:clean"
  },
  "bin": {
    "elf": "./cli/elf"
  },
  "author": "Ben Martinez-Bateman <ben@alwaysblank.org>",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@babel/core": "^7.12.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.12.7",
    "@tailwindcss/typography": "^0.3.1",
    "assets-webpack-plugin": "^6.0.4",
    "autoprefixer": "^10.0.2",
    "babel-loader": "^8.2.1",
    "chalk": "^4.1.0",
    "core-js": "^3.7.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "folder-hash": "^4.0.0",
    "image-minimizer-webpack-plugin": "^1.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "mini-css-extract-plugin": "^1.3.1",
    "mustache": "^4.0.1",
    "netlify-plugin-cache": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.10",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2",
    "webpack": "^5",
    "webpack-cli": "^4",
    "yargs": "^16.1.1"
  }
}
